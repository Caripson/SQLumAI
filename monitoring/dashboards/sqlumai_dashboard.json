{
  "title": "SQLumAI Overview",
  "timezone": "browser",
  "panels": [
    {
      "type": "stat",
      "title": "Allowed / Autocorrect / Blocks",
      "gridPos": {"x": 0, "y": 0, "w": 12, "h": 4},
      "targets": [
        {"expr": "sum(sqlumai_metric_total{key=\"allowed\"})"},
        {"expr": "sum(sqlumai_metric_total{key=\"autocorrect_suggested\"})"},
        {"expr": "sum(sqlumai_metric_total{key=\"rule\",action=\"block\"})"}
      ]
    },
    {
      "type": "timeseries",
      "title": "Bytes per write (histogram sum)",
      "gridPos": {"x": 0, "y": 4, "w": 12, "h": 8},
      "targets": [
        {"expr": "sum(rate(sqlumai_bytes_sum[1m]))"}
      ]
    },
    {
      "type": "timeseries",
      "title": "Latency ms (95th)",
      "gridPos": {"x": 0, "y": 12, "w": 12, "h": 8},
      "targets": [
        {"expr": "histogram_quantile(0.95, sum(rate(sqlumai_latency_ms_bucket[5m])) by (le))"}
      ]
    }
  ],
  "schemaVersion": 36,
  "version": 1
}
